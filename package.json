{
  "name": "my-clerk-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:unused": "npx ts-unused-exports tsconfig.json --excludePathsFromReport=\"node_modules;.next\"",
    "clean:unused": "npx unimported",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "webhook:setup": "node scripts/setup-webhook.js",
    "webhook:dev": "node scripts/dev-webhook.js",
    "webhook:manual": "node scripts/setup-webhook-manual.js",
    "webhook:demo": "node scripts/webhook-demo.js",
    "webhook:url": "node scripts/get-webhook-url.js",
    "webhook:test": "node scripts/test-webhook.js",
    "webhook:list": "node scripts/setup-webhook.js list",
    "stripe:setup": "node scripts/setup-stripe-env.js",
    "stripe:webhook": "node scripts/setup-stripe-webhook.js",
    "db:setup": "node scripts/setup-database.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test tests/visual",
    "test:performance": "vitest run tests/performance/radar-performance-simple.test.ts",
    "test:performance:e2e": "playwright test tests/e2e/radar-performance-e2e.spec.ts",
    "test:edge-cases": "vitest run tests/integration/radar-edge-cases-simple.test.ts",
    "test:edge-cases:e2e": "playwright test tests/e2e/radar-edge-cases-e2e.spec.ts",
    "test:radar:unit": "npm run test:performance && npm run test:edge-cases",
    "test:radar:e2e": "npm run test:performance:e2e && npm run test:edge-cases:e2e && npm run test:visual",
    "test:radar:all": "npm run test:radar:unit && npm run test:radar:e2e"
  },
  "dependencies": {
    "@clerk/nextjs": "6.28.0",
    "@clerk/themes": "^2.4.15",
    "@prisma/client": "6.15.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@stripe/stripe-js": "^7.9.0",
    "@types/uuid": "^10.0.0",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.11.0",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "geist": "^1.4.2",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "node-fetch": "^3.3.2",
    "prisma": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.2.1",
    "stripe": "^18.5.0",
    "svix": "^1.76.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jsdom": "^27.0.0",
    "tailwindcss": "^3.4.10",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "unimported": "^1.31.0",
    "vitest": "^3.2.4"
  }
}
